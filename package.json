{
  "name": "move-ii-orbit-standalone",
  "version": "0.0.1",
  "description": "MOVE-II orbit visualization",
  "main": "app.js",
  "scripts": {
    "postinstall": "mkdirp lib && cpx './node_modules/cesium/Build/**/*' ./lib",
    "clean": "mkdirp public & rimraf public/*",
    "prebuild": "npm run clean && cpx './lib/Cesium/**/*' ./public",
    "build": "webpack --config config/webpack.config.js --progress",
    "devserver": "webpack-dev-server --config config/webpack.config.js --progress",
    "devserverpub": "webpack-dev-server --config config/webpack.config.js --progress --host 0.0.0.0",
    "start": "npm run build && npm run devserver",
    "startpub": "npm run build && npm run devserverpub",
    "release": "npm run build -- --optimize-minimize"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "cesium": "^1.47.0",
    "jspredict-move2": "^1.0.5",
    "moment": "^2.22.2",
    "satellite.js": "^2.0.2"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
