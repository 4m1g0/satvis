{
  "name": "SatVis",
  "version": "1.0.0",
  "description": "Satellite orbit visualization with CesiumJS",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "clean": "mkdirp dist & rimraf dist/*",
    "lint": "eslint --ext .js,.vue src",
    "serve-dev": "npm run build-dev && http-server -g -c0 dist",
    "serve-prod": "npm run build && http-server -g -c0 dist",
    "start": "webpack-dev-server --config webpack.dev.js",
    "start-dev": "webpack-dev-server --config webpack.dev.js --host 0.0.0.0",
    "start-prod": "webpack-dev-server --config webpack.prod.js --host 0.0.0.0",
    "test": "ava"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "buefy": "^0.7.1",
    "cesium": "^1.52.0",
    "cesium-sensor-volumes": "^1.32.0",
    "dayjs": "^1.7.7",
    "satellite.js": "^3.0.0",
    "vue": "^2.5.18",
    "vue-multi-select": "^3.11.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.9",
    "@fortawesome/free-brands-svg-icons": "^5.6.0",
    "@fortawesome/free-regular-svg-icons": "^5.6.0",
    "@fortawesome/free-solid-svg-icons": "^5.6.0",
    "autoprefixer": "^9.4.2",
    "ava": "1.0.0-rc.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.10.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "esm": "^3.0.84",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "^0.5.1",
    "obj2gltf": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "script-loader": "^0.7.2",
    "strip-pragma-loader": "^1.0.0",
    "to-string-loader": "^1.1.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.18",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
