    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "script-loader": "^0.7.2",
    "strip-pragma-loader": "^1.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
