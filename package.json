{
  "name": "move-ii-orbit-standalone",
  "version": "0.0.1",
  "description": "MOVE-II orbit visualization",
  "scripts": {
    "build": "webpack --config webpack.prod.js --progress",
    "build-dev": "webpack --config webpack.dev.js --progress",
    "clean": "mkdirp dist & rimraf dist/*",
    "lint": "eslint src",
    "serve-release": "npm run build && http-server -g -c0 dist",
    "start": "webpack-dev-server --config webpack.dev.js --progress",
    "start-dev": "webpack-dev-server --config webpack.dev.js --progress --host 0.0.0.0",
    "start-prod": "webpack-dev-server --config webpack.prod.js --progress --host 0.0.0.0"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "cesium": "^1.48.0",
    "jspredict-move2": "^1.0.5",
    "moment": "^2.22.2",
    "satellite.js": "^2.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "eslint": "^5.4.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.2",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "script-loader": "^0.7.2",
    "strip-pragma-loader": "^1.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  }
}
