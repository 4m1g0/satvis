{
  "name": "SatVis",
  "version": "1.0.0",
  "description": "Satellite orbit visualization with CesiumJS",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --progress --config webpack.dev.js",
    "build:stats": "webpack --progress --config webpack.stats.js",
    "clean": "mkdirp dist & rimraf dist/*",
    "lint": "eslint --ext .js,.vue src",
    "serve:dev": "npm run build-dev && http-server -g -c0 dist",
    "serve:prod": "npm run build && http-server -g -c0 dist",
    "start": "webpack-dev-server --config webpack.dev.js",
    "start:dev": "webpack-dev-server --config webpack.dev.js --host 0.0.0.0",
    "start:prod": "webpack-dev-server --config webpack.prod.js --host 0.0.0.0",
    "test": "ava",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "buefy": "^0.7.2",
    "cesium": "^1.53.0",
    "cesium-sensor-volumes": "^1.32.0",
    "dayjs": "^1.8.0",
    "satellite.js": "^3.0.0",
    "vue": "^2.5.22",
    "vue-multi-select": "^3.11.1",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-brands-svg-icons": "^5.7.0",
    "@fortawesome/free-regular-svg-icons": "^5.7.0",
    "@fortawesome/free-solid-svg-icons": "^5.7.0",
    "autoprefixer": "^9.4.7",
    "ava": "1.2.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "cypress": "^3.1.4",
    "eslint": "^5.12.1",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-vue": "^5.1.0",
    "esm": "^3.1.4",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "^0.5.1",
    "obj2gltf": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "script-loader": "^0.7.2",
    "strip-pragma-loader": "^1.0.0",
    "to-string-loader": "^1.1.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all",
    "not UCAndroid >1"
  ]
}
