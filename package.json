{
  "name": "cesium-orbit",
  "version": "0.0.1",
  "description": "Satellite orbit visualization with CesiumJS",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "clean": "mkdirp dist & rimraf dist/*",
    "lint": "eslint --ext .js,.vue src",
    "serve-dev": "npm run build-dev && http-server -g -c0 dist",
    "serve-prod": "npm run build && http-server -g -c0 dist",
    "start": "webpack-dev-server --config webpack.dev.js",
    "start-dev": "webpack-dev-server --config webpack.dev.js --host 0.0.0.0",
    "start-prod": "webpack-dev-server --config webpack.prod.js --host 0.0.0.0"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "cesium": "^1.50.0",
    "cesium-sensor-volumes": "^1.32.0",
    "dayjs": "^1.7.7",
    "jspredict-move2": "^1.0.5",
    "satellite.js": "^2.0.3",
    "v-selectmenu": "^1.0.5",
    "vue": "^2.5.17",
    "vue-multi-select": "^3.11.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.4.4",
    "mkdirp": "^0.5.1",
    "obj2gltf": "^2.3.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "script-loader": "^0.7.2",
    "strip-pragma-loader": "^1.0.0",
    "to-string-loader": "^1.1.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.3",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
